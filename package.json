  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "svgo": "^2.3.1",
    "@types/svgo": "^2.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.2",
    "eslint": "^7.32.0",
    "prettier": "^2.7.1",
    "rollup": "2",
    "rollup-plugin-delete": "^2.0.0",
    "solid-js": "^1.4.7",
    "typescript": "^4.7.4",
    "vite": "^3"
  },
  "peerDependencies": {
    "solid-js": "^1",
    "vite": ">=2.4"
  }
}
